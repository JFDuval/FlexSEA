#!/bin/bash
#FlexSEA-GUI Dependencies Installation

apt-get install libfontconfig1-dev libfreetype6-dev libx11-dev libxext-dev
apt-get install libxfixes-dev libxi-dev libxrender-dev libxcb1-dev libx11-xcb-dev libxcb-glx0-dev
apt-get install libxcb-keysyms1-dev libxcb-image0-dev libxcb-shm0-dev libxcb-icccm4-dev
apt-get install libxcb-sync0-dev libxcb-xfixes0-dev libxcb-shape0-dev libxcb-randr0-dev libxcb-render-util0-dev     

echo "All dependencies should now be met."
